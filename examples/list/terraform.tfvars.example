# --------------------------------------------------------------------------------------------
# Example terraform.tfvars for the list example
# Copy this file to terraform.tfvars and customize the values
# --------------------------------------------------------------------------------------------

# Resource Group ID where your Azure Migrate project resides
# Format: /subscriptions/{subscription-id}/resourceGroups/{resource-group-name}
parent_id = "/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/my-resource-group"

project_name        = "saif-project-120126"
location            = "australiaeast"

# Migration instance type
instance_type = "VMwareToAzStackHCI"

# Optional: Specify vault ID directly (if not provided, derived from project)
# replication_vault_id = "/subscriptions/xxx/resourceGroups/xxx/providers/Microsoft.DataReplication/replicationVaults/xxx"

# Tags
tags = {
  Environment = "Test"
  Purpose     = "ListProtectedItems"
}

