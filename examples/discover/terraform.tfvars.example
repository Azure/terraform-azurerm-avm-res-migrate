# Example terraform.tfvars file
# Copy this file to terraform.tfvars and update with your actual values
#

# Resource Group ID where your Azure Migrate project resides
# Format: /subscriptions/{subscription-id}/resourceGroups/{resource-group-name}
parent_id = "/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/my-resource-group"

# Azure Migrate Project name
project_name = "my-migrate-project"

# Optional: Instance type
# instance_type = "VMwareToAzStackHCI"  # or "HyperVToAzStackHCI"

# Optional: Custom tags
# tags = {
#   Environment = "Test"
#   Purpose     = "Discovery"
#   Team        = "Migration Team"
# }

