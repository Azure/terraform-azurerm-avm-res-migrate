# Example terraform.tfvars for creating a new Azure Migrate project
# Copy this file to terraform.tfvars and update with your values

subscription_id      = "00000000-0000-0000-0000-000000000000"
resource_group_name  = "my-migrate-project-rg"
project_name         = "my-new-migrate-project"
location             = "centralus"  # Use a supported region: centralus, westus2, northeurope, westeurope, etc.
instance_type        = "VMwareToAzStackHCI"

tags = {
  Environment = "Production"
  Purpose     = "MigrateProject"
  ManagedBy   = "Terraform"
  CostCenter  = "IT-Infrastructure"
}
